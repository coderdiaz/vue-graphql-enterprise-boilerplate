    "graphql-import": "^0.7.0",
    "graphql-middleware": "^2.0.0",
    "graphql-subscriptions": "^1.0.0",
    "jsonwebtoken": "^8.4.0",
    "jwks-rsa": "^1.3.0",
    "multienv-loader": "^1.1.0",
    "prisma-binding": "^2.2.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "graphql-cli": "^2.17.0",
    "nodemon": "^1.18.0",
    "prisma": "~1.22.0"
